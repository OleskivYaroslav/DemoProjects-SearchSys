<?xml version="1.0" encoding="windows-1251" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="SearchTaskFlow">
    <default-activity>goingToSearchView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__3">
      <managed-bean-name>SearchBean</managed-bean-name>
      <managed-bean-class>SearchBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__4">
      <managed-bean-name>DBChangeBean</managed-bean-name>
      <managed-bean-class>com.ua.view.DBChangeBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <method-call id="goingToSearchView">
      <method>#{pageFlowScope.SearchBean.goToSearchView}</method>
      <outcome>
        <fixed-outcome>continue</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SearchView">
      <page>/SearchView.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>goingToSearchView</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>continue</from-outcome>
        <to-activity-id>SearchView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
